
CREATE DATABASE knk2024;

CREATE TABLE knk2024.users(
id INT PRIMARY KEY NOT NULL,
first_name VARCHAR(255),
last_name VARCHAR(255),
email VARCHAR(255),
password VARCHAR(255)
);

